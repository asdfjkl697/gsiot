RM = rm
#CC = linux-gcc
OBJS = AddressManger.o common.o EditAttrMgr.o GSIOTDevice.o GSIOTInfo.o GSIOTClient.o adhoc_example.o 
bin = gloox
CFLAGS += -I.
LIBS += -lpthread
LIBS += -lstdc++
LIBS += -lgloox

#LIBDIR = -L/lib/
all : $(bin)
$(bin) : $(OBJS)
	$(CXX) $(CFLAGS) -Wall  $(OBJS) $(LIBDIR) -o $(bin) $(LIBS)
clean :
	$(RM) -f $(OBJS)
	$(RM) -f $(bin)
